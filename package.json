{
  "name": "eslint-config-joshingmachine",
  "version": "0.6.0",
  "description": "ESLint config based on Josh Smith's preferences",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --config index.js **/*.js",
    "lint:fix": "eslint --fix --config index.js **/*.js",
    "rules:unused": "eslint-find-rules --unused ./all.js",
    "rules:unused:no-error": "eslint-find-rules --unused ./all.js --no-error",
    "rules:used": "eslint-find-rules --current ./all.js",
    "test": "npm run lint && npm run rules:unused"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshingmachine/eslint-config-joshingmachine.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "joshingmachine",
    "javascript",
    "format",
    "lint"
  ],
  "author": "Josh Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshingmachine/eslint-config-joshingmachine/issues"
  },
  "homepage": "https://github.com/joshingmachine/eslint-config-joshingmachine#readme",
  "dependencies": {
    "eslint-config-prettier": "^2.9.0",
    "prettier-config-joshingmachine": "0.2.0"
  },
  "devDependencies": {
    "eslint": "^5.3.0",
    "eslint-find-rules": "^3.2.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "prettier": "1.11.1"
  },
  "peerDependencies": {
    "eslint": "^5.3.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "prettier": "1.11.1"
  }
}
